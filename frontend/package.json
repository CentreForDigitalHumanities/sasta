{
  "name": "sasta",
  "scripts": {
    "ng": "ng",
    "start": "yarn serve",
    "build": "ng build",
    "test": "ng test --watch=true",
    "lint": "ng lint",
    "build:en": "ng build --configuration=production-en",
    "build:nl": "ng build --configuration=production-nl",
    "i18n": "ng xi18n --output-path locale",
    "serve": "ng serve --proxy-config ../proxy.conf.json --disable-host-check",
    "serve:en": "ng serve --liveReload=false --proxy-config ../proxy.conf.json --configuration=en",
    "serve:nl": "ng serve --liveReload=false --proxy-config ../proxy.conf.json --configuration=nl",
    "stop": "lsof -t -i tcp:4200 | xargs kill -9 || echo \"not running\"",
    "test-once": "ng test --watch=false",
    "watch": "ng build --prod --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.13",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~9.1.13",
    "@angular/compiler": "~9.1.13",
    "@angular/core": "~9.1.13",
    "@angular/forms": "~9.1.13",
    "@angular/localize": "~9.1.13",
    "@angular/platform-browser": "~9.1.13",
    "@angular/platform-browser-dynamic": "~9.1.13",
    "@angular/router": "~9.1.13",
    "@creativebulma/bulma-divider": "^1.1.0",
    "@fortawesome/angular-fontawesome": "~0.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.24",
    "@fortawesome/free-solid-svg-icons": "^5.11.1",
    "@types/file-saver": "^2.0.1",
    "bulma": "^0.7.5",
    "file-saver": "^2.0.2",
    "ngx-json-viewer": "^2.4.0",
    "primeicons": "^2.0.0",
    "primeng": "~9.1.2",
    "rxjs": "~6.6.7",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.15",
    "@angular/cli": "~9.1.15",
    "@angular/compiler-cli": "~9.1.13",
    "@angular/language-service": "~9.1.13",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3"
  }
}
