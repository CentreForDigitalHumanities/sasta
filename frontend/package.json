{
    "name": "sasta",
    "scripts": {
        "ng": "ng",
        "start": "yarn serve",
        "build": "ng build",
        "test": "ng test --watch=true",
        "lint": "ng lint",
        "build:en": "ng build --configuration=production-en",
        "build:nl": "ng build --configuration=production-nl",
        "i18n": "ng xi18n --output-path locale",
        "serve": "ng serve --proxy-config ../proxy.conf.json --disable-host-check",
        "serve:en": "ng serve --liveReload=false --proxy-config ../proxy.conf.json --configuration=en",
        "serve:nl": "ng serve --liveReload=false --proxy-config ../proxy.conf.json --configuration=nl",
        "stop": "lsof -t -i tcp:4200 | xargs kill -9 || echo \"not running\"",
        "test-once": "ng test --watch=false",
        "watch": "ng build --configuration production --watch"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~15.2.10",
        "@angular/cdk": "~15.2.0",
        "@angular/common": "~15.2.10",
        "@angular/compiler": "~15.2.10",
        "@angular/core": "~15.2.10",
        "@angular/forms": "~15.2.10",
        "@angular/localize": "~15.2.10",
        "@angular/platform-browser": "~15.2.10",
        "@angular/platform-browser-dynamic": "~15.2.10",
        "@angular/router": "~15.2.10",
        "@creativebulma/bulma-divider": "^1.1.0",
        "@fortawesome/angular-fontawesome": "~0.12.0",
        "@fortawesome/fontawesome-svg-core": "~6.x",
        "@fortawesome/free-solid-svg-icons": "~6.x",
        "@types/file-saver": "^2.0.1",
        "brace": "^0.11.1",
        "bulma": ">0.9.0",
        "fast-xml-parser": "^3.15.1",
        "file-saver": "^2.0.2",
        "jquery": "^3.6.0",
        "lassy-xpath": "^0.12.0",
        "ngx-json-viewer": "^2.4.0",
        "primeicons": "~6.0.0",
        "primeng": "~15.4.0",
        "rxjs": "~6.6.7",
        "ts-xpath": "^0.3.1",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~15.2.10",
        "@angular-eslint/builder": "~15.2.0",
        "@angular-eslint/eslint-plugin": "~15.2.0",
        "@angular-eslint/eslint-plugin-template": "~15.2.0",
        "@angular-eslint/schematics": "~15.2.0",
        "@angular-eslint/template-parser": "~15.2.0",
        "@angular/cli": "~15.2.10",
        "@angular/compiler-cli": "~15.2.10",
        "@angular/language-service": "~15.2.10",
        "@types/jasmine": "^3.7.1",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "4.28.2",
        "@typescript-eslint/parser": "4.28.2",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsdoc": "^39.6.4",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "jasmine-core": "^3.7.1",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.3.16",
        "karma-chrome-launcher": "~3.1.0",
