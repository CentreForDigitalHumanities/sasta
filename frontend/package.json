{
  "name": "sasta",
  "scripts": {
    "ng": "ng",
    "start": "yarn serve",
    "build": "ng build --base-href=/static/",
    "test": "ng test --watch=true",
    "lint": "ng lint",
    "build:en": "ng build --configuration=production-en",
    "build:nl": "ng build --configuration=production-nl",
    "i18n": "ng xi18n --output-path locale",
    "serve": "ng serve --proxy-config ../proxy.conf.json",
    "serve:en": "ng serve --liveReload=false --proxy-config ../proxy.conf.json --configuration=en",
    "serve:nl": "ng serve --liveReload=false --proxy-config ../proxy.conf.json --configuration=nl",
    "stop": "lsof -t -i tcp:4200 | xargs kill -9 || echo \"not running\"",
    "test-once": "ng test --watch=false",
    "watch": "ng build --configuration production --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.7",
    "@angular/cdk": "^15.2.7",
    "@angular/common": "^15.2.7",
    "@angular/compiler": "^15.2.7",
    "@angular/core": "^15.2.7",
    "@angular/forms": "^15.2.7",
    "@angular/localize": "^15.2.7",
    "@angular/platform-browser": "^15.2.7",
    "@angular/platform-browser-dynamic": "^15.2.7",
    "@angular/router": "^15.2.7",
    "@creativebulma/bulma-divider": "^1.1.0",
    "@fortawesome/angular-fontawesome": "^0.12.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@types/file-saver": "^2.0.5",
    "brace": "^0.11.1",
    "bulma": "^0.9.4",
    "fast-xml-parser": "^3.15.1",
    "file-saver": "^2.0.5",
    "jquery": "^3.6.4",
    "lassy-xpath": "^0.15.1",
    "lodash": "^4.17.21",
    "primeicons": "^6.0.1",
    "primeng": "^15.3.0",
    "rxjs": "^6.6.7",
    "ts-xpath": "^0.4.0",
    "tslib": "^2.5.0",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.2.6",
    "@angular/compiler-cli": "^15.2.7",
    "@angular/language-service": "^15.2.7",
    "@types/jasmine": "^4.3.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^16.x",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^41.1.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "^4.6.0",
